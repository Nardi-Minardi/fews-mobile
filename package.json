{
  "name": "FFEWFMS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "start:dev": "cross-env ENVFILE=.env.dev react-native start",
    "start:staging": "cross-env ENVFILE=.env.staging react-native start",
    "start:prod": "cross-env ENVFILE=.env.prod react-native start",
    "android": "react-native run-android",
    "android:dev": "cross-env ENVFILE=.env.dev react-native run-android",
    "android:staging": "cross-env ENVFILE=.env.staging react-native run-android",
    "android:prod": "cross-env ENVFILE=.env.prod react-native run-android",
    "ios": "react-native run-ios",
    "ios:dev": "cross-env ENVFILE=.env.dev react-native run-ios",
    "ios:staging": "cross-env ENVFILE=.env.staging react-native run-ios",
    "ios:prod": "cross-env ENVFILE=.env.prod react-native run-ios",
    "build:android:dev": "cross-env ENVFILE=.env.dev cd android && ./gradlew assembleRelease",
    "build:android:staging": "cross-env ENVFILE=.env.staging cd android && ./gradlew assembleRelease",
    "build:android:prod": "cross-env ENVFILE=.env.prod cd android && ./gradlew assembleRelease",
    "build:ios:dev": "cross-env ENVFILE=.env.dev react-native run-ios --configuration Release",
    "build:ios:staging": "cross-env ENVFILE=.env.staging react-native run-ios --configuration Release",
    "build:ios:prod": "cross-env ENVFILE=.env.prod react-native run-ios --configuration Release",
    "lint": "eslint .",
    "test": "jest",
    "test:dev": "cross-env ENVFILE=.env.dev jest",
    "test:staging": "cross-env ENVFILE=.env.staging jest",
    "test:prod": "cross-env ENVFILE=.env.prod jest"
  },
